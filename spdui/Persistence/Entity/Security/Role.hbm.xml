<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0">
  <class name="Dndp.Persistence.Entity.Security.Role, Dndp.Persistence" table="ROLES">
    <id name="Id" column="ROLE_ID" type="Int32" unsaved-value="0">
      <generator class="identity" />
    </id>
    <property name="Name" column="ROLE_NAME" type="String(50)" unique="true" />
    <property name="Description" column="DESCRIPTION" type="String(200)" />
  </class>
</hibernate-mapping>

