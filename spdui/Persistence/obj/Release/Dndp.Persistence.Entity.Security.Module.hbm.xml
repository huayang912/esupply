<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0">
  <class name="Dndp.Persistence.Entity.Security.Module, Dndp.Persistence" table="MODULES">
    <id name="Id" column="MODULE_ID" type="Int32" unsaved-value="0">
      <generator class="identity" />
    </id>
    <property name="Name" column="MODULE_NAME" type="String(50)" />
    <property name="Description" column="DESCRIPTION" type="String(200)" />
    <property name="SourceFile" column="SOURCE_FILE" type="String(200)" />
  </class>
</hibernate-mapping>

